{
  "name": "colab4food",
  "version": "0.0.0",
  "description": "This template should help get you started developing with Vue 3 in Vite.",
  "author": "Gabriel Sousa",
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "dev": "start cmd /k vite",
    "api": "start cmd /k nodemon ./API/app.js",
    "Api_FORCE": "nodemon ./API/appForceUpdateBd.js",
    "frondEnd_Backend": "(/k  npm run dev --start) |  (/k  npm run api --start)",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "test": "./src/scripts/mainScript.bat",
    "seeds": "cd ./API && npx sequelize-cli db:seed:all"
  },
